.length .list {
  margin: 20rpx 20rpx;
  background-color: #fff;
}
.length .list .list-item {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.length .list .list-item .name {
  -webkit-box-flex: 2;
  -webkit-flex: 2;
          flex: 2;
  text-align: center;
  border-right: 4rpx solid #f3f3f3;
  border-bottom: 4rpx solid #f3f3f3;
  font-size: 30rpx;
  line-height: 200%;
  color: #666;
}
.length .list .list-item .input {
  -webkit-box-flex: 3;
  -webkit-flex: 3;
          flex: 3;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  border-bottom: 4rpx solid #f3f3f3;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.length .list .list-item .input input {
  padding-left: 20rpx;
  font-size: 30rpx;
  color: #666;
}

