.container .tellphone {
  border: 1px solid #45b787;
  width: 500rpx;
  height: 100rpx;
  border-radius: 30rpx;
  margin: 180rpx auto 30rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  text-align: center;
}
.container .btn {
  width: 150px;
  margin: 0 auto;
}
.container .showInfo {
  width: 600rpx;
  height: 250rpx;
  background-color: #ccc;
  margin: 80rpx auto;
  border-radius: 30rpx;
  padding: 20rpx 20rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  line-height: 50rpx;
}

